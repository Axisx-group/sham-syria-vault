{
  "name": "sham-syria-vault",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:mobile": "vite --config vite.mobile.config.ts",
    "build:mobile": "tsc && vite build --config vite.mobile.config.ts",
    "preview:mobile": "vite preview --config vite.mobile.config.ts",
    "dev:client": "vite --config vite.client.config.ts",
    "build:client": "tsc && vite build --config vite.client.config.ts",
    "preview:client": "vite preview --config vite.client.config.ts",
    "dev:client-mobile": "vite --config vite.client-mobile.config.ts",
    "build:client-mobile": "tsc && vite build --config vite.client-mobile.config.ts",
    "preview:client-mobile": "vite preview --config vite.client-mobile.config.ts",
    "cap:sync:client-mobile": "npx cap sync --config capacitor-client-mobile.config.ts",
    "cap:add:android:client-mobile": "npx cap add android --config capacitor-client-mobile.config.ts",
    "cap:add:ios:client-mobile": "npx cap add ios --config capacitor-client-mobile.config.ts",
    "cap:run:android:client-mobile": "npx cap run android --config capacitor-client-mobile.config.ts",
    "cap:run:ios:client-mobile": "npx cap run ios --config capacitor-client-mobile.config.ts",
    "cap:build:android:client-mobile": "npm run build:client-mobile && npx cap sync android --config capacitor-client-mobile.config.ts && npx cap run android --config capacitor-client-mobile.config.ts",
    "cap:build:ios:client-mobile": "npm run build:client-mobile && npx cap sync ios --config capacitor-client-mobile.config.ts && npx cap run ios --config capacitor-client-mobile.config.ts",
    "build:full-client-apk": "npm run build:client-mobile && npx cap sync --config capacitor-client-mobile.config.ts && npx cap open android --config capacitor-client-mobile.config.ts",
    "cap:sync": "npx cap sync --config capacitor-mobile.config.ts",
    "cap:add:android": "npx cap add android --config capacitor-mobile.config.ts",
    "cap:add:ios": "npx cap add ios --config capacitor-mobile.config.ts",
    "cap:run:android": "npx cap run android --config capacitor-mobile.config.ts",
    "cap:run:ios": "npx cap run ios --config capacitor-mobile.config.ts",
    "cap:build:android": "npm run build:mobile && npx cap sync android --config capacitor-mobile.config.ts && npx cap run android --config capacitor-mobile.config.ts",
    "cap:build:ios": "npm run build:mobile && npx cap sync ios --config capacitor-mobile.config.ts && npx cap run ios --config capacitor-mobile.config.ts",
    "build:full-apk": "npm run build:mobile && npx cap sync --config capacitor-mobile.config.ts && npx cap open android --config capacitor-mobile.config.ts"
  },
